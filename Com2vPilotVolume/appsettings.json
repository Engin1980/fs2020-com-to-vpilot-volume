{
  "AppSimCon": {
    "NumberOfComs": 3,
    "ConnectionTimerInterval": 5000,
    "ComVolumeVarPrefix": "COM VOLUME:",
    "ComTransmitVarPrefix": "COM TRANSMIT:"
  },
  "AppVPilot": {
    "ConnectionTimerInterval": 5000,
    "ReadVolumeTimerInterval": 250,
    "VolumeMultiplier": 0.20

  },
  "MainWindow": {
    "StartupWindowSize": [ 1200, 450 ],
    "ShowSimpleAdjustButtons": true
  },
  "Logging": {
    "MainWindowOut": {
      "Rules": [
        {
          "Pattern": ".*Mixer",
          "Level": "info"
        },
        {
          "Pattern": ".+",
          "Level": "info"
        }
      ]
    }
  }
}